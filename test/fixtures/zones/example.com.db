$ORIGIN example.com.
$TTL 1h
@      IN SOA   ns1.example.com. hostmaster.example.com. (
                  2016010100 ; serial
                  1h         ; refresh
                  15m        ; retry
                  4w         ; expire
                  1h         ; nxdomain ttl
                )

       IN NS      ns1.example.com.
       IN NS      ns2.example.com.
       IN NS      ns3.example.com.

; Name servers
ns1    IN A       10.0.0.10
ns2    IN A       10.0.0.11
ns3    IN A       10.0.0.12

; non server domain hosts
fred   IN A       10.0.0.13
joe    IN A       10.0.0.14
